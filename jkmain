pipeline{
   agent any

   stages{
      stage(git checkout) {
        steps {
        echo 'git clone the code'
        git branch: 'main', url: 'https://github.com/BuddoluAnoopKumar/mindcircuit15d.git' 
     }
}
     stage(build) {
        steps {
        echo 'maven build '
         sh 'mvn clean install'
       }
}

}
