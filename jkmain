pipeline{
   agent any

   stages{
      stage(CHECKOUT) {
        steps {
        echo 'git clone the code'
        git branch: 'main', url: 'https://github.com/BuddoluAnoopKumar/mindcircuit15d.git' 
     }
   }
     stage(BUILD) {
        steps {
        echo 'maven build'
         sh 'mvn clean install'
       }
     }

}
